{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api-docs(.*)",
        "dest": "index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "index.ts"
      }
    ],
    "env": {
      "BASE_URL": "https://voyage-travel-be.vercel.app/api"
    }
  }